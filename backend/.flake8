[flake8]
max-line-length = 88
extend-ignore = E203, E266, E501, W503, F403, F401
exclude = 
    .git,
    __pycache__,
    migrations,
    venv,
    venv_py312,
    .tox,
    dist,
    .eggs,
    *.egg,
    build,
    .pytest_cache
max-complexity = 10
select = B,C,E,F,W,T4,B9
per-file-ignores = 
    __init__.py:F401
    settings/*.py:F405,E501
    */tests/*:F811