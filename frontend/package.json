{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage",
    "test:clients": "npx jest --testMatch='**/*clients*/**/*.test.{ts,tsx}' --testMatch='**/clientsStore.test.ts'",
    "test:e2e": "playwright test",
    "test:e2e:watch": "npx jest --config tests/e2e/setup/jest.e2e.config.js --watch",
    "test:bff": "npm run test:e2e -- --testMatch='**/bff-*.test.ts'",
    "test:bff:dashboard": "npm run test:e2e -- --testMatch='**/bff-dashboard.test.ts'",
    "test:bff:auth": "npm run test:e2e -- --testMatch='**/bff-auth-context.test.ts'",
    "test:bff:availability": "npm run test:e2e -- --testMatch='**/bff-availability.test.ts'",
    "test:bff:fallback": "npm run test:e2e -- --testMatch='**/bff-fallback.test.ts'",
    "test:integration": "npm run test:e2e -- --testMatch='**/integration/**/*.test.ts'",
    "test:performance": "npm run test:e2e -- --testMatch='**/*performance*.test.ts' --verbose",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:storybook": "test-storybook",
    "test:visual": "ts-node scripts/visual-regression.ts",
    "test:visual:update": "ts-node scripts/visual-regression.ts --update-baseline",
    "test:playwright": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:debug": "playwright test --debug",
    "test:playwright:headed": "playwright test --headed",
    "test:playwright:report": "playwright show-report",
    "analyze": "ANALYZE=true next build",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:ui": "playwright test --ui",
    "e2e:report": "playwright show-report",
    "e2e:install": "playwright install",
    "test:e2e:ci": "playwright test --reporter=line"
  },
